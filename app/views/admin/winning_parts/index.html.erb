<section class="content-header">
  <h1>
   Winning Parts
    <small>List</small>
  </h1>
  <ol class="breadcrumb">
    <li><%= link_to "<i class='fa fa-wheelchair'></i> WinningPart".html_safe , admin_winning_parts_path  %></li>
    <li class="active">List</li>
  </ol>
  <br />
  <%#= render 'shared/error_messages' %>
</section>

<section class="content">
<div class="row">
  <div class="col-md-12">
    <div class="box box-primary">

      <div class="box-header">
        <h3 class="box-title">
           <%= link_to 'New WinningPart', new_admin_winning_part_path, class: "label label-primary" %>
        </h3>
        <br />
        <div class="box-body">
          <div class="table-responsive">
            <div id="example_wrapper" class="dataTables_wrapper" role="grid">
             
                
           <table id="game"  class="display" >
                <thead>
                    <th>Text_panel</th>
                    <th>Number of Element</th>
                    <th>Part COlor</th>
                   
                    <th colspan="3"></th>
                 </thead>
                <tbody>
                  <% @winning_parts.each do |winning_part| %>
                    <tr>
                      <td><%= winning_part.text_panel %></td>
                      <td><%= winning_part.num_of_element %></td>
                      <td><%= winning_part.part_color %></td>
                      <td><%= link_to 'Show', admin_winning_part_path(winning_part) %></td>
                      <td><%= link_to 'Edit', edit_admin_winning_part_path(winning_part) %></td>
                      <td><%= link_to 'Destroy', admin_winning_part_path(winning_part), method: :delete, data: { confirm: 'Are you sure?' } %></td>
                    </tr>
                  <% end %>

                </tbody>       
              </table>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
</section>

