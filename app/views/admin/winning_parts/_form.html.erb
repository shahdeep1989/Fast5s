<div class="row">
  <div class="col-md-12">
    <div class="box box-primary">
      <div class="box-header">
        <% if @winning_part.new_record? %>
          <h3 class="box-title">New Winning Part</h3>
        <% else %>
          <h3 class="box-title">Edit Winning Part</h3>
        <% end %>
        <h3 class="box-title" style="float: right;"><%if request.url.include?("edit")%>  <%= link_to "Show", admin_winning_part_path(@winning_part), class: "label label-primary" %><% end  %>  <%= link_to "Back", admin_winning_parts_path, class: "label label-primary" %></h3>
          <%=  form_for([:admin,@winning_part], :html => {:id => "newPatientForm" , :multipart => true }  , :class => "bootstrap-validator-form") do |f| %>
            <div class="box-body col-md-6">

                <div class="form-group">
                  <label>Game</label>
                  <%= f.select :game_id ,options_from_collection_for_select(Game.all,:id, :name ,f.object.winning_part_game_id), {:include_blank => false},{:class => "form-control diagnosis_cls"} %>
                </div>

                <div class="form-group">
                  <label><%= f.label :text_panel%></label>
                  <%= f.text_field :text_panel, :class => "form-control" %>
                </div>

                <div class="form-group">
                  <label><%= f.label "Number Of Element" %></label>
                  <%= f.number_field :num_of_element , :class => "form-control"  %>
                </div>

                <div class="form-group">
                  <label>Color</label>
                   <%= f.text_field :part_color, :class => "form-control" %>
                </div>
                              
                <div class="box-footer col-md-6">
                  <%= f.submit class: "btn btn-primary" %>
                </div>
            </div>
          <% end %>
      </div>
    </div>
  </div>
</div>

<script type="text/javascript">
      $("#winning_part_part_color").colorpicker();
      $("#winning_part_game_id").select2();
</script>
